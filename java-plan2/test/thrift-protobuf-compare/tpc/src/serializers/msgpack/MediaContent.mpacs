(class MediaContent
	(package serializers.msgpack)
	(field image (array (class Image
			(field uri string)
			(field title string)
			(field width int)
			(field height int)
			(field size int))))
	(field media (class Media
			(field uri string)
			(field title string)
			(field width int)
			(field height int)
			(field format string)
			(field duration long)
			(field size long)
			(field bitrate int)
			(field person (array string))
			(field player int)
			(field copyright string)))
	)
